{
  "name": "template-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "dev:entities": "tsx tools/cli.ts watch --verbose",
    "predev": "convex dev --until-success && convex dashboard",
    "build": "tsc -b && vite build",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "kit": "tsx tools/cli.ts",
    "kit:generate": "tsx tools/cli.ts generate",
    "kit:watch": "tsx tools/cli.ts watch",
    "kit:dev": "tsx tools/cli.ts dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:entity-parser": "vitest run tests/entity-parser.test.ts",
    "test:schema-generator": "vitest run tests/schema-generator.test.ts",
    "test:integration": "vitest run tests/integration.test.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.25.0",
    "@clerk/react-router": "^1.8.8",
    "@clerk/themes": "^2.4.4",
    "@mantine/charts": "^8.2.2",
    "@mantine/core": "^8.2.2",
    "@mantine/hooks": "^8.2.2",
    "@mantine/modals": "^8.2.2",
    "@mantine/notifications": "^8.2.2",
    "@mantine/nprogress": "^8.2.2",
    "@mantine/spotlight": "^8.2.2",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-router": "^1.130.9",
    "@tanstack/react-router-devtools": "^1.130.9",
    "convex": "^1.23.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.7.1",
    "recharts": "^3.1.0",
    "svix": "^1.69.0",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/router-plugin": "^1.130.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@types/prettier": "^3.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.2.4",
    "chokidar": "^4.0.1",
    "commander": "^12.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "glob": "^11.0.0",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.0.14",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  },
